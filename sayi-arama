import java.util.Scanner;
public class Main {
    public static void main(String[] args) {

        int [] sayiDizisi=new int[]{1,3,5,67,88};
        int aranacakSayi;
        boolean varMi=false;
        Scanner arama=new Scanner(System.in);

        System.out.print("Bulmak istediğiniz sayı :");
        aranacakSayi=arama.nextInt();

        for (int sayi : sayiDizisi) {
            if (sayi == aranacakSayi) {
                varMi = true;
                break;
            }
        }
            if (varMi=true){
                System.out.print("sayı vardır");
            }
            else {
                System.out.print("Sayı yoktur.");
            }
        }
    }
